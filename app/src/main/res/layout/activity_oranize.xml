<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/ftwo"
    tools:context=".ui.OranizeActivity">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingBottom="@dimen/dp_10">


        <ImageView
            android:id="@+id/personal_head"
            android:layout_width="0dp"
            android:layout_height="@dimen/dp_196"
            android:focusableInTouchMode="true"
            android:focusable="true"
            android:scaleType="fitXY"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/personal_bg" />

        <ImageView
            android:id="@+id/personal_back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingTop="@dimen/dp_55"
            android:paddingLeft="@dimen/dp_13"
            android:paddingRight="@dimen/dp_20"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/personal_back" />

        <ImageView
            android:id="@+id/personal_share"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/dp_13"
            app:layout_constraintBottom_toBottomOf="@+id/personal_back"
            android:visibility="gone"
            app:layout_constraintEnd_toEndOf="parent"
            app:srcCompat="@drawable/personal_share" />
        <ImageView
            android:id="@+id/personal_more"
            android:layout_width="wrap_content"
            android:visibility="gone"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/dp_13"
            android:padding="@dimen/dp_13"
            app:layout_constraintBottom_toBottomOf="@+id/personal_back"
            app:layout_constraintEnd_toEndOf="parent"
            app:srcCompat="@drawable/personal_ddd" />


        <androidx.constraintlayout.widget.Guideline
            android:id="@+id/guideline11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_begin="@dimen/dp_171" />


        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintLayout3"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/dp_14"
            android:layout_marginRight="@dimen/dp_14"
            android:animateLayoutChanges="true"
            android:background="@drawable/personal_grad"
            android:padding="@dimen/dp_10"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/guideline11">

            <TextView
                android:id="@+id/personal_namee"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:drawableLeft="@drawable/personal_sun"
                android:drawablePadding="@dimen/dp_3"
                android:focusable="true"
                android:focusableInTouchMode="true"
                android:paddingTop="@dimen/dp_5"
                android:text="心理咨询机构："
                android:textColor="@color/home_txt"
                android:textStyle="bold"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <EditText
                android:id="@+id/personal_name"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:background="@null"
                android:paddingTop="@dimen/dp_5"
                android:textColor="@color/home_txt"
                android:paddingRight="@dimen/dp_8"
                android:textSize="14sp"
                android:textStyle="bold"
                app:layout_constraintEnd_toStartOf="@+id/personal_onesure"
                app:layout_constraintStart_toEndOf="@+id/personal_namee"
                app:layout_constraintTop_toTopOf="parent"
                tools:text="帅帅帅帅帅帅帅帅帅帅帅帅" />

            <TextView
                android:id="@+id/personal_jigouu"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:drawablePadding="@dimen/dp_3"
                android:text="地址："
                android:textColor="@color/home_txt"
                android:textSize="12sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/personal_name" />

            <EditText
                android:id="@+id/personal_jigou"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:background="@null"
                android:textColor="@color/home_txt"
                android:textSize="12sp"
                android:paddingRight="@dimen/dp_8"
                app:layout_constraintEnd_toStartOf="@+id/personal_id"
                app:layout_constraintStart_toEndOf="@+id/personal_jigouu"
                app:layout_constraintTop_toTopOf="@+id/personal_jigouu"
                tools:text="哈哈省哈哈哈市胡给对方区寻常心理" />

            <TextView
                android:id="@+id/personal_id"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="@color/home_txt"
                android:textSize="12sp"
                app:layout_constraintBaseline_toBaselineOf="@+id/personal_jigouu"
                app:layout_constraintEnd_toEndOf="parent"
                tools:text="ID:12313" />

            <ImageView
                android:id="@+id/personal_oneedt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="@dimen/dp_20"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/personal_edt" />

            <TextView
                android:id="@+id/personal_onesure"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/personal_sure"
                android:gravity="center"
                android:text="确定"
                android:textColor="@color/white"
                android:visibility="gone"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/personal_onerecy"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:visibility="gone"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/personal_jigouu"
                tools:listitem="@layout/item_talk" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintLayout4"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:animateLayoutChanges="true"
            android:background="@drawable/personal_white"
            android:padding="@dimen/dp_10"
            app:layout_constraintEnd_toEndOf="@+id/constraintLayout3"
            app:layout_constraintStart_toStartOf="@+id/constraintLayout3"
            app:layout_constraintTop_toBottomOf="@+id/constraintLayout3">

            <TextView
                android:id="@+id/textView63"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="优秀咨询师"
                android:textColor="@color/threethree"
                android:textSize="16sp"
                android:textStyle="bold"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/textView64"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:text="机构简介"
                android:textColor="@color/threethree"
                android:textSize="16sp"
                android:textStyle="bold"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/personal_tworecy" />

            <ImageView
                android:id="@+id/showgoods_edt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="@dimen/dp_20"
                app:layout_constraintBottom_toBottomOf="@+id/textView63"
                app:layout_constraintEnd_toEndOf="parent"
                app:srcCompat="@drawable/personal_edt" />

            <TextView
                android:id="@+id/showgoods_edtsure"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/personal_sure"
                android:gravity="center"
                android:text="确定"
                android:textColor="@color/white"
                app:layout_constraintTop_toTopOf="@+id/textView63"
                android:visibility="gone"
                app:layout_constraintEnd_toEndOf="parent"
                app:srcCompat="@drawable/personal_edt" />


            <ImageView
                android:id="@+id/personal_threeedt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="@dimen/dp_20"
                app:layout_constraintBottom_toBottomOf="@+id/textView64"
                app:layout_constraintEnd_toEndOf="parent"
                app:srcCompat="@drawable/personal_edt" />

            <TextView
                android:id="@+id/personal_threesure"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/personal_sure"
                android:gravity="center"
                android:text="确定"
                android:textColor="@color/white"
                android:visibility="gone"
                app:layout_constraintBottom_toBottomOf="@+id/textView64"
                app:layout_constraintEnd_toEndOf="parent"
                app:srcCompat="@drawable/personal_edt" />

            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/personal_tworecy"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView63" />

            <EditText
                android:id="@+id/personal_geren"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:background="@null"
                android:hint="请输入或编辑个人简介"
                android:textColor="@color/threethree"
                android:textSize="12sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textView64" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/con_fuwu"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_10"
            android:layout_marginBottom="@dimen/dp_10"
            android:animateLayoutChanges="true"
            android:background="@drawable/personal_white"
            android:paddingLeft="@dimen/dp_10"
            android:paddingTop="@dimen/dp_10"
            android:paddingRight="@dimen/dp_10"
            app:layout_constraintEnd_toEndOf="@+id/constraintLayout3"
            app:layout_constraintStart_toStartOf="@+id/constraintLayout3"
            app:layout_constraintTop_toBottomOf="@+id/constraintLayout4">

            <TextView
                android:id="@+id/tt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="服务项目"
                android:textColor="@color/threethree"
                android:textSize="16sp"
                android:textStyle="bold"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />


            <ImageView
                android:id="@+id/personal_fouredt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:visibility="gone"
                android:paddingLeft="@dimen/dp_20"
                app:layout_constraintBottom_toBottomOf="@+id/tt"
                app:layout_constraintEnd_toEndOf="parent"
                app:srcCompat="@drawable/personal_edt" />

            <TextView
                android:id="@+id/personal_foursure"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/personal_sure"
                android:gravity="center"
                android:text="确定"
                android:textColor="@color/white"
                android:visibility="gone"
                app:layout_constraintBottom_toBottomOf="@+id/tt"
                app:layout_constraintEnd_toEndOf="parent"
                app:srcCompat="@drawable/personal_edt" />

            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/personal_thiredrecy"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/tt" />

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/con_bt"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:animateLayoutChanges="true"
            android:background="@drawable/personal_white"
            android:padding="@dimen/dp_10"
            app:layout_constraintEnd_toEndOf="@+id/constraintLayout3"
            app:layout_constraintStart_toStartOf="@+id/constraintLayout3"
            app:layout_constraintTop_toBottomOf="@+id/con_fuwu">

            <TextView
                android:id="@+id/textVie3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="相册/资料"
                android:textColor="@color/threethree"
                android:textSize="16sp"
                android:textStyle="bold"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/personal_fiveedt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="@dimen/dp_20"
                app:layout_constraintBottom_toBottomOf="@+id/textVie3"
                app:layout_constraintEnd_toEndOf="parent"
                app:srcCompat="@drawable/personal_edt" />

            <TextView
                android:id="@+id/personal_fivesure"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/personal_sure"
                android:gravity="center"
                android:text="确定"
                android:textColor="@color/white"
                app:layout_constraintTop_toTopOf="@+id/textVie3"
                android:visibility="gone"
                app:layout_constraintEnd_toEndOf="parent"
                app:srcCompat="@drawable/personal_edt" />


            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/personal_photorecy"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/textVie3" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/con_user"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="10dp"
            android:layout_marginTop="20dp"
            android:visibility="gone"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/con_bt"
            tools:visibility="visible">

            <TextView
                android:id="@+id/look_btshare"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dp_15"
                android:layout_marginRight="@dimen/dp_25"
                android:layout_marginBottom="@dimen/dp_25"
                android:background="@drawable/mine_grad"
                android:gravity="center"
                android:padding="@dimen/dp_10"
                android:text="分享"
                android:textColor="@color/login_txt"
                android:textSize="16sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/look_bttalk"
                app:layout_constraintStart_toStartOf="parent" />

            <TextView
                android:id="@+id/look_bttalk"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dp_25"
                android:layout_marginRight="@dimen/dp_15"
                android:background="@drawable/mine_grad"
                android:gravity="center"
                android:padding="@dimen/dp_10"
                android:text="私聊"
                android:textColor="@color/login_txt"
                android:textSize="16sp"
                app:layout_constraintBottom_toBottomOf="@+id/look_btshare"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/look_btshare"
                app:layout_constraintTop_toTopOf="@+id/look_btshare" />


        </androidx.constraintlayout.widget.ConstraintLayout>


    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.core.widget.NestedScrollView>