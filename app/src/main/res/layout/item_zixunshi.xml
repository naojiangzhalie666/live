<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginBottom="@dimen/dp_8"
    android:background="@drawable/bg_corner_whitea"
    android:padding="@dimen/dp_15">

    <TextView
        android:id="@+id/textVie45"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp_8"
        android:layout_marginTop="15dp"
        android:focusable="true"
        android:focusableInTouchMode="true"
        android:text="姓名"
        android:textColor="@color/black_oneeight"
        android:textSize="13sp"
        app:layout_constraintStart_toEndOf="@+id/textView26"
        app:layout_constraintTop_toBottomOf="@+id/textView42" />

    <EditText
        android:id="@+id/item_zx_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@null"
        android:gravity="right"
        android:hint="请填写咨询师的真实姓名"
        android:textColor="@color/black_oneeight"
        android:textSize="11sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/textVie45"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textVie26"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="*"
        android:textColor="@color/red"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textVie45" />

    <View
        android:id="@+id/textView43"
        android:layout_width="0dp"
        android:layout_height="@dimen/dp_1_5"
        android:layout_marginTop="15dp"
        android:background="@drawable/line_dott"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.727"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textVie45" />

    <TextView
        android:id="@+id/manone_t"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp_6"
        android:layout_marginTop="15dp"
        android:focusable="true"
        android:focusableInTouchMode="true"
        android:text="上传至少1位咨询师形象照"
        android:textColor="@color/black_oneeight"
        android:textSize="13sp"
        app:layout_constraintStart_toEndOf="@+id/textView28"
        app:layout_constraintTop_toBottomOf="@+id/textView43" />

    <TextView
        android:id="@+id/textView28"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/dp_8"
        android:text="*"
        android:textColor="@color/red"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/manone_t" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="@dimen/dp_114"
        android:layout_height="@dimen/dp_152"
        android:layout_marginLeft="@dimen/dp_15"
        android:layout_marginTop="15dp"
        android:scaleType="centerCrop"
        android:src="@drawable/person_sample"
        app:layout_constraintEnd_toStartOf="@+id/textView41"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_chainStyle="spread_inside"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/manone_t" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ruzhu_next"
        app:layout_constraintBottom_toBottomOf="@+id/imageView8"
        app:layout_constraintEnd_toStartOf="@+id/textView41"
        app:layout_constraintStart_toEndOf="@+id/imageView8"
        app:layout_constraintTop_toTopOf="@+id/imageView8" />

    <TextView
        android:id="@+id/textView41"
        android:layout_width="@dimen/dp_114"
        android:layout_height="@dimen/dp_152"
        android:layout_marginRight="@dimen/dp_15"
        android:background="@color/ftwo"
        app:layout_constraintBottom_toBottomOf="@+id/imageView8"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/imageView8"
        app:layout_constraintTop_toTopOf="@+id/imageView8" />

    <ImageView
        android:id="@+id/manone_xxadd"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:scaleType="centerCrop"
        android:src="@drawable/ruzhu_add"
        app:layout_constraintBottom_toBottomOf="@+id/textView41"
        app:layout_constraintEnd_toEndOf="@+id/textView41"
        app:layout_constraintStart_toStartOf="@+id/textView41"
        app:layout_constraintTop_toTopOf="@+id/textView41" />

    <ImageView
        android:id="@+id/item_zx_xxpic"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toBottomOf="@+id/textView41"
        app:layout_constraintEnd_toEndOf="@+id/textView41"
        app:layout_constraintStart_toStartOf="@+id/textView41"
        app:layout_constraintTop_toTopOf="@+id/textView41" />

    <TextView
        android:id="@+id/textView46"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="参考案例"
        android:textColor="@color/nineninenine"
        android:textSize="11sp"
        app:layout_constraintEnd_toEndOf="@+id/imageView8"
        app:layout_constraintStart_toStartOf="@+id/imageView8"
        app:layout_constraintTop_toBottomOf="@+id/imageView8" />

    <TextView
        android:id="@+id/textView53"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="上传照片"
        android:textColor="@color/nineninenine"
        android:textSize="11sp"
        app:layout_constraintEnd_toEndOf="@+id/textView41"
        app:layout_constraintStart_toStartOf="@+id/textView41"
        app:layout_constraintTop_toBottomOf="@+id/textView41" />


    <TextView
        android:id="@+id/textView45"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp_8"
        android:layout_marginTop="10dp"
        android:text="资质审核"
        android:textColor="@color/black_oneeight"
        android:textSize="13sp"
        app:layout_constraintStart_toEndOf="@+id/textView26"
        app:layout_constraintTop_toBottomOf="@+id/textView46" />

    <TextView
        android:id="@+id/textView26"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="*"
        android:textColor="@color/red"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView45" />

    <TextView
        android:id="@+id/item_zx_sttm"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:minWidth="@dimen/dp_80"
        android:gravity="center"
        android:background="@drawable/bg_corner_ft"
        android:drawableRight="@drawable/mine_next"
        android:drawablePadding="@dimen/dp_5"
        android:padding="@dimen/dp_8"
        android:text="2019年5月6日"
        android:textColor="@color/home_txt"
        android:textSize="12sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView45" />

    <View
        android:id="@+id/textView31"
        android:layout_width="@dimen/dp_10"
        android:layout_height="@dimen/dp_1_5"
        android:layout_marginStart="4dp"
        android:background="@drawable/bg_circle_stoke_gray"
        app:layout_constraintBottom_toBottomOf="@+id/item_zx_sttm"
        app:layout_constraintStart_toEndOf="@+id/item_zx_sttm"
        app:layout_constraintTop_toTopOf="@+id/item_zx_sttm" />

    <TextView
        android:id="@+id/item_zx_edtm"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:minWidth="@dimen/dp_80"
        android:gravity="center"
        android:background="@drawable/bg_corner_ft"
        android:drawableRight="@drawable/mine_next"
        android:drawablePadding="@dimen/dp_5"
        android:padding="@dimen/dp_8"
        android:text="2019年5月6日"
        android:textColor="@color/home_txt"
        android:textSize="12sp"
        app:layout_constraintBaseline_toBaselineOf="@+id/item_zx_sttm"
        app:layout_constraintStart_toEndOf="@+id/textView31" />

    <TextView
        android:id="@+id/tv1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_10"
        android:background="@drawable/bg_corner_ft"
        android:gravity="left"
        android:lineSpacingExtra="@dimen/dp_4"
        android:paddingLeft="@dimen/dp_12"
        android:paddingTop="@dimen/dp_16"
        android:paddingBottom="@dimen/dp_16"
        android:text="参考示例：\n接受XXX心理咨询培训，获取XXX心理咨询师证书。"
        android:textColor="@color/nineninenine"
        android:textSize="11sp"
        app:layout_constraintTop_toBottomOf="@+id/item_zx_sttm" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/item_zx_zzrecy"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_10"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv1" />


</androidx.constraintlayout.widget.ConstraintLayout>