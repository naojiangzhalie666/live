<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/bg_top_corner_white">
    <TextView
        android:id="@+id/dialog_int_cancel"
        android:layout_width="wrap_content"
        android:text="取消"
        android:layout_alignParentLeft="true"
        android:paddingLeft="@dimen/dp_16"
        android:paddingBottom="@dimen/dp_14"
        android:paddingTop="@dimen/dp_14"
        android:paddingRight="@dimen/dp_20"
        android:layout_height="wrap_content"/>
    <TextView
        android:id="@+id/dialog_int_sure"
        android:layout_width="wrap_content"
        android:text="确定"
        android:paddingLeft="@dimen/dp_20"
        android:paddingBottom="@dimen/dp_14"
        android:paddingTop="@dimen/dp_14"
        android:paddingRight="@dimen/dp_16"
        android:layout_alignParentRight="true"
        android:layout_height="wrap_content"/>
    <View
        android:id="@+id/line"
        android:layout_below="@+id/dialog_int_sure"
        android:layout_width="match_parent"
        android:background="@color/ftwo"
        android:layout_height="@dimen/dp_0_5"/>


    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/botselect_recy"
        android:layout_width="match_parent"
        android:paddingLeft="@dimen/dp_5"
        android:paddingRight="@dimen/dp_5"
        android:paddingTop="@dimen/dp_14"
        android:layout_height="wrap_content"
        android:layout_below="@+id/line"
        android:paddingBottom="@dimen/dp_10" />


</RelativeLayout>